{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions" : [
        {
            "name": "client",
            "image": "ajmeraabhishek/complex-client",
            "hostname": "client",
            "essential": false,
            "memory": 100
        },
        {
            "name": "server",
            "image": "ajmeraabhishek/complex-server",
            "hostname": "api",
            "essential": false,
            "memory": 100
        },
        {
            "name": "worker",
            "image": "ajmeraabhishek/complex-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 100
        },
        {
            "name": "nginx",
            "image": "ajmeraabhishek/complex-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 100,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
        "links": ["client","server"]
        }        
    ]
}